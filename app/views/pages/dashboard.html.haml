.dashboard.gray.lighten-3.ng-scope
  .row.no-gutter
    .col-sm-12.col-md-12.col-lg-9
      %h2 My Dashboard
      .card.z-depth-3
        %table.table.striped.clients
          %tr
            %th Client
            %th Interactions
            %th Appointments
            %th Actions
          = render partial: 'clients/client', collection: @my_clients, client: :client
        = paginate @my_clients
